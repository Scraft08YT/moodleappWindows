{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "moodle-desktop": {
            "projectType": "application",
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular/build:application",
                    "options": {
                        "outputPath": "dist/moodle-desktop",
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": ["zone.js"],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "src/assets",
                                "output": "assets"
                            }
                        ],
                        "styles": ["src/styles.scss"],
                        "scripts": [],
                        "stylePreprocessorOptions": {
                            "includePaths": ["src/styles"]
                        }
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "40kB",
                                    "maximumError": "60kB"
                                }
                            ],
                            "outputHashing": "all",
                            "optimization": {
                                "scripts": true,
                                "styles": {
                                    "minify": true,
                                    "inlineCritical": false,
                                    "removeSpecialComments": true
                                },
                                "fonts": false
                            }
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular/build:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "moodle-desktop:build:production"
                        },
                        "development": {
                            "buildTarget": "moodle-desktop:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    },
    "cli": {
      "analytics": false
    }
}
